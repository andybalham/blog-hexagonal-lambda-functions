{
  "name": "blog-hexagonal-lambda-functions",
  "version": "1.0.0",
  "description": "Source code for blog post on hexagonal lambda functions.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint-and-fix": "eslint ./src --ext .ts --fix",
    "build": "npm run lint && tsc",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test-coverage": "nyc npm run test",
    "cdk-synth-data-access-test": "tsc && cdk synth DataAccessTestStack > .cdk-synth.yml",
    "cdk-deploy-data-access-test": "tsc && cdk deploy DataAccessTestStack --hotswap",
    "cdk-destroy-data-access-test": "tsc && cdk destroy DataAccessTestStack",
    "cdk-synth-function-test": "tsc && cdk synth FunctionTestStack > .cdk-synth.yml",
    "cdk-deploy-function-test": "tsc && cdk deploy FunctionTestStack --hotswap",
    "cdk-destroy-function-test": "tsc && cdk destroy FunctionTestStack",
    "cdk-synth-application": "tsc && cdk synth ApplicationStack > .cdk-synth.yml",
    "cdk-deploy-application": "tsc && cdk deploy ApplicationStack",
    "cdk-hotswap-application": "tsc && cdk deploy ApplicationStack --hotswap",
    "cdk-destroy-application": "tsc && cdk destroy ApplicationStack",
    "cdk-synth-data-storage": "tsc && cdk synth DataStorageStack > .cdk-synth.yml",
    "cdk-deploy-data-storage": "tsc && cdk deploy DataStorageStack",
    "cdk-synth": "tsc && cdk synth > .cdk-synth.yml",
    "cdk-destroy-all": "tsc && cdk destroy --all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybalham/blog-hexagonal-lambda-functions.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andybalham/blog-hexagonal-lambda-functions/issues"
  },
  "homepage": "https://github.com/andybalham/blog-hexagonal-lambda-functions#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.81",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "aws-sdk": "^2.958.0",
    "esbuild": "^0.12.17",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@andybalham/cdk-cloud-test-kit": "^1.0.3",
    "aws-cdk-lib": "^2.24.1",
    "constructs": "^10.1.10",
    "nanoid": "^3.3.4"
  }
}
